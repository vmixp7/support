<div id="wrapper">

  <%- partial("../layout/left") %>

    <div id="page-wrapper">
      <div class="row">
        <div class="col-lg-12">
          <h1 class="page-header">所在地列表</h1>
        </div>
        <!-- /.col-lg-12 -->
      </div>
      <div class="row">
        <div class="col-lg-12">
          <a onClick="swalAddSubmit()" class="btn btn-primary" href="javascript:;" role="button">
            <%= sails.__({ phrase: 'create', locale: sails.config.myconf.lang[0] }) %>
          </a>
        </div>
      </div>
      <br>
      <!-- /.row -->
      <div class="row">
        <div class="col-lg-12">

          <div class="panel panel-default">
            <div class="panel-heading">
              <%=bar_title%>
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">

              <div class="dataTable_wrapper">
                <table class="table table-striped table-bordered table-hover" id="dataTables-example" width="100%">
                  <thead>
                    <tr>
                      <th>流水號</th>
                      <th>問題類別</th>
                      <th>狀態</th>
                      <th>資料更新時間</th>
                      <th>啟用/停用</th>
                      <th>編輯</th>
                    </tr>
                  </thead>
                </table>
              </div>
              <!-- /.table-responsive -->
            </div>
            <!-- /.panel-body -->
          </div>
          <!-- /.panel -->
        </div>
        <!-- /.col-lg-12 -->
      </div>
    </div>
    <!-- /#page-wrapper -->
</div>
<script>
  $(document).ready(function() {
    var column_array = [{
        "mData": "nation_pk"
      },
      {
        "mData": "nation_name"
      },
      {
        "mData": "nation_name_en"
      },
      {
        "mData": "customer_group_name"
      },
      {
        "mData": "updatedAt"
      },
      {
        "mData": "finalModify",
        orderable: false
      }
    ];
    create_ajaxtable('dataTables-example', '<%=viewLink%>', column_array);

  });
</script>
